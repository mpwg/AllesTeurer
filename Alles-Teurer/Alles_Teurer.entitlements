<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>aps-environment</key>
	<string>development</string>
	<key>com.apple.developer.aps-environment</key>
	<string>development</string>
	
	<!-- CloudKit for optional sync -->
	<key>com.apple.developer.icloud-container-identifiers</key>
	<array>
		<string>iCloud.eu.mpwg.allesteurer</string>
	</array>
	<key>com.apple.developer.icloud-services</key>
	<array>
		<string>CloudKit</string>
	</array>
	
	<!-- App Groups for widget data sharing -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.eu.mpwg.allesteurer.shared</string>
	</array>
	
	<!-- Siri and Shortcuts -->
	<key>com.apple.developer.siri</key>
	<true/>
	
	<!-- Associated Domains for Universal Links -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:allesteurer.mpwg.eu</string>
	</array>
	
	<!-- Vision Framework and ML -->
	<key>com.apple.developer.machine-learning-compute</key>
	<true/>
</dict>
</plist>
